From d66a66a602a2d4cd999526691c26a90340ea0c07 Mon Sep 17 00:00:00 2001
From: Neil Armstrong <narmstrong@baylibre.com>
Date: Tue, 18 Apr 2017 14:44:51 +0000
Subject: [PATCH 4/4] ARM64: dts: meson-gxbb-odroidc2: Add ACME nodes

Signed-off-by: Neil Armstrong <narmstrong@baylibre.com>
---
 .../arm64/boot/dts/amlogic/meson-gxbb-odroidc2.dts | 149 +++++++++++++++++++++
 1 file changed, 149 insertions(+)

diff --git a/arch/arm64/boot/dts/amlogic/meson-gxbb-odroidc2.dts b/arch/arm64/boot/dts/amlogic/meson-gxbb-odroidc2.dts
index ee77c3d..a5f1337 100644
--- a/arch/arm64/boot/dts/amlogic/meson-gxbb-odroidc2.dts
+++ b/arch/arm64/boot/dts/amlogic/meson-gxbb-odroidc2.dts
@@ -53,6 +53,7 @@
 	
 	aliases {
 		serial0 = &uart_AO;
+		i2c1 = &i2c_A;
 	};
 
 	chosen {
@@ -131,6 +132,13 @@
 		regulator-max-microvolt = <3300000>;
 	};
 
+	acme_mux_switch: acme_mux_switch@0 {
+		compatible = "regulator-fixed";
+		regulator-name = "acme_i2c_mux";
+		gpio = <&pca9534 0 GPIO_ACTIVE_LOW>;
+		regulator-always-on;
+	};
+
 	emmc_pwrseq: emmc-pwrseq {
 		compatible = "mmc-pwrseq-emmc";
 		reset-gpios = <&gpio BOOT_9 GPIO_ACTIVE_LOW>;
@@ -186,6 +194,147 @@
 	status = "okay";
 	pinctrl-0 = <&i2c_a_pins>;
 	pinctrl-names = "default";
+	clock-frequency = <400000>;
+
+	pca9534: pca9534@21 {
+		compatible = "pca9534";
+		reg = <0x21>;
+		#gpio-cells = <2>;
+	};
+
+	pca9535: pca9535@20 {
+		compatible = "pca9535";
+		reg = <0x20>;
+		#gpio-cells = <2>;
+	};
+
+	acme_probe0: acme_probe0@40 {
+		compatible = "ti,ina226";
+		reg = <0x40>;
+		gpio = <&pca9535 1 0>;
+	};
+
+	acme_probe_eeprom0: acme_probe_eeprom0@50 {
+		compatible = "at,24c02";
+		reg = <0x50>;
+	};
+
+	acme_probe_serial0: acme_probe_serial0@58 {
+		compatible = "at,24cs02";
+		reg = <0x58>;
+	};
+
+	acme_probe1: acme_probe1@41 {
+		compatible = "ti,ina226";
+		reg = <0x41>;
+		gpio = <&pca9535 3 0>;
+	};
+
