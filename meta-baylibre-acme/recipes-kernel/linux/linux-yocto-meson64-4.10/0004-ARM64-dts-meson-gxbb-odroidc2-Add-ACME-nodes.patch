+		reg = <0x53>;
+	};
+
+	acme_probe_serial3: acme_probe_serial3@5b {
+		compatible = "at,24cs02";
+		reg = <0x5b>;
+	};
+
+	acme_probe4: acme_probe4@44 {
+		compatible = "ti,ina226";
+		reg = <0x44>;
+		gpio = <&pca9535 9 0>;
